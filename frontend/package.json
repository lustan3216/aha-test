{
  "name": "@aha-test/frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "PORT=8000 NODE_ENV=development umi dev",
    "build": "NODE_ENV=production umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-layout": "^6.5.0",
    "@rollbar/react": "^0.11.0",
    "@umijs/preset-react": "^2.1.2",
    "antd": "^4.19.3",
    "axios": "^0.26.1",
    "dayjs": "^1.11.0",
    "js-cookie": "^3.0.1",
    "lodash.camelcase": "^4.3.0",
    "react": "17.x",
    "react-countdown-hook": "^1.1.1",
    "react-dom": "17.x",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.2.2",
    "rollbar": "^2.24.1",
    "umi": "^3.5.21"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/react": "^17.0.0",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/react-dom": "^17.0.0",
    "@types/react-facebook-login": "^4.1.4",
    "gts": "^3.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "4.4.4",
    "yorkie": "^2.0.0"
  }
}
